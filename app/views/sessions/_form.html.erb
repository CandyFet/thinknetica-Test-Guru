<%= form_with url: sessions_path, local: true do |f| %>
  <% unless flash_message.blank? %>
    <%= flash_message %>
  <% end %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email, id: :email%>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password, id: :password%>
  </p> 

  <p> 
    <%= f.submit 'Enter'%>
  </p>  
<% end %>